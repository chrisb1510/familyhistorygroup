<% @content = require('../../../content/aboutContent').Content %>

<% grantHelper = (grant) => %>
    <h2><%- "#{grant}" %></h2>
    <p><%- @content['Grants']["#{grant}"] %></p>
    
<% end %>

<article id="Grants">
    <header>
        <h1>Grants</h1>
    </header>

    <% for grant of @content['Grants'] : %>
        <%- grantHelper grant %>
    <% end %>
        
</article>
   
