<% @content = require('../../../content/homeContent').Content.Home %>

<% articleHelper = (paragraph) => %>
    <article id="<%- paragraph %>">
        <header>
            <h1><%- paragraph %></h1>
        </header>
        <p><%- @content["#{paragraph}"] %></p>
    </article>
<% end %>

<section id="Home">
    <header>
        <h1>Home</h1>
    </header>
    
    <% for paragraph of @content : %>
        <%- articleHelper paragraph %>
    <% end %>
</section>
    
